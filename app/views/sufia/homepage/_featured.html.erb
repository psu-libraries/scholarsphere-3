<% presenter = featured.presenter %>
<li class="featured-item" data-id="<%= presenter.id %>">
  <div class="main row">
    <div class="col-sm-3">
      <%= render_thumbnail_tag presenter, { width: 90, alt: '', "aria-hidden" => true } %>
    </div>
    <div class="col-sm-9">
      <%= render 'sufia/homepage/featured_fields', featured: presenter %>
    </div>
  </div>
</li>
